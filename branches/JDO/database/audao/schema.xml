<?xml version="1.0" encoding="utf-8"?>
<database xmlns="http://www.spoledge.com/audao" version="1.0">

	<config>
		<factory>
			<create-params default="false" direct="true"/>
		</factory>
	</config>

	<tables>
		<table name="SatelliteElementSet">
			<auto-find/>
			<columns>
				<column name="catalogNumber">
					<type>long</type>
					<pk/>
				</column>
				<column name="name">
					<type max-length="30">String</type>
					<not-null/>
					<edit/>
				</column>
				<column name="line1">
					<type max-length="80">String</type>
					<not-null/>
					<edit/>
				</column>
				<column name="line2">
					<type max-length="80">String</type>
					<not-null/>
					<edit/>
				</column>
				<column name="setNumber">
					<type>long</type>
					<not-null/>
					<edit/>
				</column>
				<column name="createdDate">
					<type>Date</type>
					<not-null/>
					<edit/>
				</column>
				<column name="updatedDate">
					<type>Date</type>
					<edit/>
				</column>
			</columns>
			<indexes>
				<index name="catalogNumber_idx">
					<unique/>
					<columns>
						<column name="catalogNumber"/>
					</columns>
				</index>
			</indexes>
		</table>
	</tables>
</database>

